public class Date extends Object {
  
  private int day = 0;
  private int month = 0;
  private int year = 0;
  
  public Date(int day, int month, int year) {
    this.day = day;
    this.month = month;
    this.year = year;
  }
              
  public int getDay() {
    return day;
  }
  
  public int getMonth() {
    return month;
  }
  
  public int getYear() {
    return year;
  }
  
  public boolean compareTo(Date date) {
    if (this.getDay() < date.getDay())
      return true;
    return true;
  }
}